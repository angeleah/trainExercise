%table
  %tr
    %th Train Line
    %th Route
    %th Run Number
    %th Operator ID
  -@schedule.each do |data|
    %tr
      %td #{data.line}
      %td #{data.route}
      %td #{data.run_number}
      %td #{data.operator}

%h3 Import a CSV
%form(action='/import' method='POST' enctype='multipart/form-data')
  %input(type='file' name='myfile')
  %input(type='submit' value='Upload' )
